{
  "name": "mimmi20/laminas-paginator-adapter-laminasdb",
  "description": "laminas-db adapters for laminas-paginator",
  "license": "MIT",
  "keywords": [
    "laminas",
    "pagination",
    "db"
  ],
  "support": {
    "issues": "https://github.com/mimmi20/laminas-laminas-paginator-adapter-db/issues",
    "source": "https://github.com/mimmi20/laminas-laminas-paginator-adapter-db",
    "docs": "https://docs.laminas.dev/laminas-laminas-paginator-adapter-db/"
  },
  "require": {
    "php": "~8.1.0 || ~8.2.0 || ~8.3.0 || ~8.4.0",
    "ext-mbstring": "*",
    "laminas/laminas-db": "^2.20.0",
    "laminas/laminas-paginator": "^2.19.0",
    "laminas/laminas-servicemanager": "^3.22.1",
    "psr/container": "^1.1.2"
  },
  "require-dev": {
    "ext-ctype": "*",
    "ext-dom": "*",
    "ext-intl": "*",
    "ext-simplexml": "*",
    "ext-tokenizer": "*",
    "ext-xml": "*",
    "ext-xmlwriter": "*",
    "infection/infection": "^0.29.8",
    "laminas/laminas-i18n": "^2.29.0",
    "laminas/laminas-inputfilter": "^2.32.0",
    "laminas/laminas-modulemanager": "^2.17.0",
    "laminas/laminas-validator": "^2.64.1",
    "mimmi20/coding-standard": "^5.3.2",
    "nikic/php-parser": "^5.4.0",
    "phpstan/extension-installer": "^1.4.3",
    "phpstan/phpstan": "^2.1.12",
    "phpstan/phpstan-deprecation-rules": "^2.0.2",
    "phpstan/phpstan-phpunit": "^2.0.6",
    "phpunit/phpunit": "^10.5.45",
    "rector/rector": "^2.0.12",
    "rector/type-perfect": "^2.0.2",
    "shipmonk/composer-dependency-analyser": "^1.8.2",
    "symfony/process": "^6.4.19",
    "symplify/phpstan-rules": "^14.6.4",
    "tomasvotruba/cognitive-complexity": "^1.0.0",
    "tomasvotruba/type-coverage": "^2.0.2",
    "tomasvotruba/unused-public": "^2.0.1"
  },
  "autoload": {
    "psr-4": {
      "Laminas\\Paginator\\Adapter\\LaminasDb\\": "src//"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LaminasTest\\Paginator\\Adapter\\LaminasDb\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "infection/extension-installer": true,
      "phpstan/extension-installer": true
    },
    "lock": false,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laminas": {
      "component": "Laminas\\Paginator\\Adapter\\LaminasDb",
      "config-provider": "Laminas\\Paginator\\Adapter\\LaminasDb\\ConfigProvider"
    }
  }
}
